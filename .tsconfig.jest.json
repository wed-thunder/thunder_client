{
  "extends": "./tsconfig",
  "compilerOptions": {
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strict": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true
  }
}
